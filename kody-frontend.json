{
  "project": "invoice-go-frontend",
  "name": "vuexy",
  "rootDir": "./../invoice-go/frontend/",
  "page": [
    {
      "name": "home",
      "template": "page.vue.template"
    }
  ],
  "authPage": [
    {
      "name": "Login",
      "template": "Login.vue.template",
      "outputDir": "",
      "data": {
        "appName": "فوترة",
        "title": "تبدأ المغامرة هنا 🚀",
        "description": "اجعل إدارة فواتيرك سهلة وممتعة!"
      }
    },
    {
      "name": "Register",
      "template": "register/Register.vue.template",
      "outputDir": "register",
      "data": {
        "appName": "فوترة",
        "title": "تبدأ المغامرة هنا 🚀",
        "description": "اجعل إدارة فواتيرك سهلة وممتعة!"
      }
    },
    {
      "name": "ResetPassword",
      "template": "ResetPassword.vue.template",
      "outputDir": "",
      "data": {
        "appName": "فوترة"
      }
    },
    {
      "name": "ForgotPassword",
      "template": "ForgotPassword.vue.template",
      "outputDir": "",
      "data": {
        "appName": "فوترة",
        "title": "تبدأ المغامرة هنا 🚀",
        "description": "اجعل إدارة فواتيرك سهلة وممتعة!"
      }
    }
  ],
  "router": [
    {
      "template": "index.js.template"
    }
  ],
  "route": [
    {
      "name": "auth",
      "template": "auth.js.template"
    },
    {
      "name": "modules",
      "template": "modules.js.template",
      "routes": [
        {
          "name": "invoice",
          "children": [
            {
              "path": "/invoice",
              "routeName": "list"
            },
            {
              "path": "/invoice/create",
              "routeName": "add"
            },
            {
              "path": "/invoice/:id",
              "routeName": "view"
            },
            {
              "path": "/invoice/:id/edit",
              "routeName": "edit"
            }
          ]
        },
        {
          "name": "customer",
          "children": [
            {
              "path": "/customer",
              "routeName": "list"
            }
          ]
        }
      ]
    },
    {
      "name": "pages",
      "template": "pages.js.template",
      "routes": [
        {
          "name": "home",
          "path": "/",
          "routeName": "home"
        }
      ]
    }
  ],
  "navigation": [
    {
      "layout": "vertical",
      "template": "index.js.template",
      "routes": [
        {
          "header": "الرئيسية",
          "icon": "Home",
          "routeName": "home"
        },
        {
          "header": "الفواتير",
          "icon": "Home",
          "children": [
            {
              "title": "الفواتير",
              "routeName": "invoices-list"
            },
            {
              "title": "إنشاء فاتورة",
              "routeName": "invoices-add"
            },
            {
              "title": "العملاء",
              "routeName": "customers-list"
            }
          ]
        },
        {
          "header": "المصاريف",
          "icon": "Home",
          "children": [
            {
              "title": "المصاريف",
              "routeName": "invoices-list"
            },
            {
              "title": "إنشاء فاتورة",
              "routeName": "invoices-add"
            },
            {
              "title": "المزودين",
              "routeName": "customers-list"
            }
          ]
        },
        {
          "header": "الإعدادات",
          "icon": "Home",
          "children": [
            {
              "title": " إعدادات الشركة",
              "routeName": "invoices-list"
            },
            {
              "title": "إعدادات الحساب",
              "routeName": "invoices-add"
            }
          ]
        }
      ]
    },
    {
      "layout": "horizontal",
      "template": "index.js.template",
      "routes": [
        {
          "header": "الرئيسية",
          "icon": "Home",
          "routeName": "home"
        },
        {
          "header": "الفواتير",
          "icon": "Home",
          "children": [
            {
              "title": "الفواتير",
              "routeName": "invoices-list",
              "icon": "Home"
            },
            {
              "title": "إنشاء فاتورة",
              "routeName": "invoices-add",
              "icon": "Home"
            },
            {
              "title": "العملاء",
              "routeName": "customers-list",
              "icon": "User"
            }
          ]
        },
        {
          "header": "المصاريف",
          "icon": "Home",
          "children": [
            {
              "title": "المصاريف",
              "routeName": "invoices-list",
              "icon": "Home"
            },
            {
              "title": "إنشاء فاتورة",
              "routeName": "invoices-add",
              "icon": "Home"
            },
            {
              "title": "المزودين",
              "routeName": "customers-list",
              "icon": "User"
            }
          ]
        },
        {
          "header": "الإعدادات",
          "icon": "Home",
          "children": [
            {
              "title": " إعدادات الشركة",
              "routeName": "invoices-list",
              "icon": "Home"
            },
            {
              "title": "إعدادات الحساب",
              "routeName": "invoices-add",
              "icon": "Home"
            }
          ]
        }
      ]
    }
  ],
  "asset": [
    {
      "path": "",
      "filename": ".env",
      "template": ".env.template",
      "data": {
        "locale": "ar",
        "fallbackLocale": "en",
        "baseUrl": "https://api.invoice-go.noqta.tn/api/v1",
        "localBaseUrl": "http://localhost:8000/api/v1"
      }
    },
    {
      "path": "",
      "filename": "themeConfig.js",
      "template": "themeConfig.js.template",
      "data": {
        "appName": "InvoiceGo",
        "isRTL": true
      }
    },
    {
      "path": "src/libs",
      "filename": "axios.js",
      "template": "src/libs/axios.js.template"
    },
    {
      "path": "src/auth",
      "filename": "utils.js",
      "template": "src/auth/utils.js.template"
    },
    {
      "path": "src/libs/acl",
      "filename": "routeProtection.js",
      "template": "src/libs/acl/routeProtection.js.template"
    },
    {
      "path": "src/layouts/components",
      "filename": "Navbar.vue",
      "template": "src/layouts/components/Navbar.vue.template"
    },
    {
      "path": "src/@core/mixins/ui",
      "filename": "feedback.js",
      "template": "src/@core/mixins/ui/feedback.js.template"
    },
    {
      "path": "public",
      "filename": "index.html",
      "template": "public/index.html.template",
      "data": {
        "title": "InvoiceGo",
        "BASE_URL": "<%= BASE_URL %>"
      }
    },
    {
      "path": "src/@core/scss/base/bootstrap-extended",
      "filename": "_variables.scss",
      "template": "src/@core/scss/base/bootstrap-extended/_variables.scss.template",
      "data": {
        "fontFamily": "'Readex Pro', sans-serif"
      }
    }
  ],
  "module": [
    {
      "name": "invoice",
      "templateFolder": "invoice"
    },
    {
      "name": "customer",
      "templateFolder": "module",
      "data": {
        "columns": [
          {
            "name": "name",
            "label": "الاسم",
            "type": "text"
          },
          {
            "name": "email",
            "label": "البريد الإلكتروني",
            "type": "text"
          },
          {
            "name": "phone",
            "label": "رقم الهاتف",
            "type": "text"
          },
          {
            "name": "address",
            "label": "العنوان",
            "type": "text"
          }
        ],
        "options": [
          {
            "name": "role",
            "items": [
              { "label": "مدرير", "value": "admin" },
              { "label": "شركة", "value": "company" }
            ]
          },
          {
            "name": "plan",
            "items": [
              { "label": "Basic", "value": "basic" },
              { "label": "Company", "value": "company" },
              { "label": "Enterprise", "value": "enterprise" },
              { "label": "Team", "value": "team" }
            ]
          },
          {
            "name": "status",
            "items": [
              { "label": "نشط", "value": "active" },
              { "label": "غير نشط", "value": "inactive" }
            ]
          }
        ]
      }
    }
  ]
}
